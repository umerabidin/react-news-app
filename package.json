{
  "name": "myfirstproject",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "clear": "cd android && gradlew clean && cd ../",
    "prebuild": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build": "cd android && gradlew assembleDebug && cd ../",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev-android": "react-native run-android --variant=devDebug --appIdSuffix dev"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.12",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/stack": "^6.3.21",
    "@reduxjs/toolkit": "^2.2.1",
    "@types/react": "~18.2.45",
    "expo": "^49.0.23",
    "expo-checkbox": "~2.4.0",
    "expo-image-picker": "~14.3.2",
    "expo-secure-store": "^12.8.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.10",
    "react-native-config": "^1.5.1",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-onboarding-swiper": "^1.2.0",
    "react-native-paper": "^5.12.3",
    "react-native-pro-ui": "^1.1.1",
    "react-native-reanimated": "^3.7.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-sensitive-info": "^5.5.8",
    "react-native-webview": "^13.8.1",
    "react-redux": "^9.1.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "rn-secure-storage": "^3.0.1",
    "typescript": "^5.3.0"
